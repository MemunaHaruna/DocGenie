{
  "name": "docgenie",
  "version": "1.0.0",
  "description": "A document management system to help users organize documents",
  "main": "server.js",
  "scripts": {
    "build": "webpack -p",
    "start:dev": "NODE_ENV=development webpack -p & nodemon server.js",
    "start": "NODE_ENV=production node server.js",
    "debug": "nodemon --inspect server.js",
    "test": "NODE_ENV=test PORT=3333 nyc --reporter=html --reporter=text mocha -r mock-local-storage --compilers js:babel-core/register --colors ./server/tests/*/*.spec.js ./client/tests/*/*.spec.js --require ./server/tests/init.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-mharuna/DocGenie.git"
  },
  "keywords": [
    "document",
    "api"
